int L2=2;
int L3=3;
int L4=4;
int L5=5;
int L6=6;
int L7=7;
int L8=8;
int L9=9;
int L10=10;
int L11=11;

int tiempo=5;
int ciclo=35;

void setup(){
  pinMode(L2,OUTPUT);
  pinMode(L3,OUTPUT);
  pinMode(L4,OUTPUT);
  pinMode(L5,OUTPUT);
  pinMode(L6,OUTPUT);
  pinMode(L7,OUTPUT);
  pinMode(L8,OUTPUT);
  pinMode(L9,OUTPUT);
  pinMode(L10,OUTPUT);
  pinMode(L11,OUTPUT);

}

void loop(){

  //LLAMADO DE FUNCIONES PARA CADA UNA DE LAS LETRAS
    for(int i = 0; i < ciclo; i++)
    {
     Ll(); 
    }

    for(int i = 0; i < ciclo; i++)
    {
     La(); 
    }

    for(int i = 0; i < ciclo; i++)
    {
     Lb(); 
    }

    for(int i = 0; i < ciclo; i++)
    {
     espacio(); 
    }

      for(int i = 0; i < ciclo; i++)
    {
     Le(); 
    }

    for(int i = 0; i < (ciclo); i++)
    {
     Ll(); 
    }

    for(int i = 0; i < (ciclo); i++)
    {
     Le(); 
    }

      for(int i = 0; i < (ciclo); i++)
    {
     Lc(); 
    }

      for(int i = 0; i < ciclo; i++)
    {
     Lt(); 
    }

      for(int i = 0; i < (ciclo); i++)
    {
     Lr(); 
    }
    
      for(int i = 0; i < ciclo; i++)
    {
     Lo(); 
    }

    for(int i = 0; i < (ciclo); i++)
    {
     Ln(); 
    }

      for(int i = 0; i < (ciclo); i++)
    {
     Li(); 
    }

      for(int i = 0; i < (ciclo); i++)
    {
     Lc(); 
    }
    
      for(int i = 0; i < (ciclo); i++)
    {
     La(); 
    }

      for(int i = 0; i < (ciclo); i++)
    {
     espacio(); 
    }

      for(int i = 0; i < (ciclo); i++)
    {
     N4(); 
    }


      for(int i = 0; i < (ciclo*2); i++)
    {
     espacio(); 
    }


  // Escribir aquí el texto
}

/////////////////FUNCIONES/////////////////////////////////////////////////////////////////////////////////////////

void La(){

  digitalWrite(L2, LOW); 
  digitalWrite(L3, LOW);
  digitalWrite(L4, HIGH);
  digitalWrite(L5, LOW);
  digitalWrite(L6, HIGH);
  digitalWrite(L7, LOW); 
  digitalWrite(L8, HIGH); 
  digitalWrite(L9, HIGH); 
  digitalWrite(L10, HIGH);
  digitalWrite(L11, LOW);
  delay(tiempo);

  digitalWrite(L2, LOW); 
  digitalWrite(L3, LOW);
  digitalWrite(L4, LOW);
  digitalWrite(L5, LOW);
  digitalWrite(L6, LOW);
  digitalWrite(L7, LOW); 
  digitalWrite(L8, LOW); 
  digitalWrite(L9, LOW); 
  digitalWrite(L10, LOW);
  digitalWrite(L11, LOW);
  delay(tiempo);

  digitalWrite(L2, HIGH); 
  digitalWrite(L3, HIGH);
  digitalWrite(L4, HIGH);
  digitalWrite(L5, HIGH);
  digitalWrite(L6, HIGH);
  digitalWrite(L7, HIGH); 
  digitalWrite(L8, LOW); 
  digitalWrite(L9, LOW); 
  digitalWrite(L10, LOW);
  digitalWrite(L11, HIGH);
  delay(tiempo);

  digitalWrite(L2, LOW); 
  digitalWrite(L3, LOW);
  digitalWrite(L4, LOW);
  digitalWrite(L5, LOW);
  digitalWrite(L6, LOW);
  digitalWrite(L7, LOW); 
  digitalWrite(L8, LOW); 
  digitalWrite(L9, LOW); 
  digitalWrite(L10, LOW);
  digitalWrite(L11, LOW);
  delay(tiempo);
}

