  digitalWrite(L4, LOW);
  digitalWrite(L5, LOW);
  digitalWrite(L6, LOW);
  digitalWrite(L7, LOW); 
  digitalWrite(L8, LOW); 
  digitalWrite(L9, LOW); 
  digitalWrite(L10, LOW);
  digitalWrite(L11, LOW);
  delay(tiempo);

  digitalWrite(L2, LOW); 
  digitalWrite(L3, LOW);
  digitalWrite(L4, LOW);
  digitalWrite(L5, LOW);
  digitalWrite(L6, HIGH);
  digitalWrite(L7, LOW); 
  digitalWrite(L8, LOW); 
  digitalWrite(L9, HIGH); 
  digitalWrite(L10, HIGH);
  digitalWrite(L11, LOW);
  delay(tiempo);

  digitalWrite(L2, LOW); 
  digitalWrite(L3, LOW);
  digitalWrite(L4, LOW);
  digitalWrite(L5, LOW);
  digitalWrite(L6, LOW);
  digitalWrite(L7, LOW); 
  digitalWrite(L8, LOW); 
  digitalWrite(L9, LOW); 
  digitalWrite(L10, LOW);
  digitalWrite(L11, LOW);
  delay(tiempo);
}

